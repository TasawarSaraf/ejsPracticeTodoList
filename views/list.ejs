<%- include("header") -%>
    <div class="box" id="heading">
        <h1> <%= listTitle %></h1>
    </div>

    <div class="box">
        <% for(let i=0; i<todoList.length; i++) { %>
            <div class="item">
                <input type="checkbox">
                <p><%= todoList[i] %></p>
            </div>
        <% } %>


        <form class="box" action="/" method="post">
            <input type="text" name="todoItem" placeholder="New Item" autocomplete="off">
            <button type="submit" name="list" value=<%=listTitle%>>+</button>
        </form>
    </div>

    

 <%-include("footer")-%>